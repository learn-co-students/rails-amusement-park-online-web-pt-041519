
<% options_array = [] %>
<% User.all.each do |user| %>
    <% options_array << user.name %>
<% end %>


<%= form_tag url: '/signin' do %>
<%= label_tag "user_name" %>
<%= select_tag(:user_name, options_for_select(options_array)) %>
<%= label_tag "password" %>
<%= password_field_tag "password" %>
<%= submit_tag "Sign In"%>
<% end %>

